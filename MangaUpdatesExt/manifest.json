{
    
    "manifest_version": 3,
    "name": "Manga Updates Unread Counter",
    "version": "2.0.0",
    "description": "A extension to count unread series in  MangaUpdates and sorts them accordingly",
    "author": "marxseny@gmail.com",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
    "content_scripts": [
        {
            "matches": ["https://www.mangaupdates.com/*"],
            "js": [ 
                "script.js"
            ]
        }  
    ]
}
